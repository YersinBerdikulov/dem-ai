{
  "expo": {
    "name": "dem-AI",
    "slug": "dem-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#03174C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.demal.app",
      "buildNumber": "1.0.0",
      "config": {
        "usesNonExemptEncryption": false,
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.445082810786-1lmm42tv3phjj4rbc9nnh59dtugk0743"
        }
     
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "fb1131088858800791",
              "demai",
              "com.googleusercontent.apps.445082810786-1lmm42tv3phjj4rbc9nnh59dtugk0743"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "fbapi",
          "fb-messenger-share",
          "fbauth2",
          "fbshareextension",
          "googlechromes",
          "comgooglemaps"
        ],
       
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#03174C"
      },
      "package": "com.demal.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "demai",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "fb1131088858800791"
            }
          ]
        }
      ]
    },
    "scheme": "demai",
    "plugins": [
      [
      
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to set your profile picture."
        }
      ],
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "f9007a81-3fe2-4690-9610-6353f59ee041"
      }
    },
    "owner": "yersultan12",
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
