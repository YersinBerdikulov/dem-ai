{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "prebuildCommand": "npm install && rm -rf android/app/src/main/java/com/demai/app/MainActivity.kt android/app/src/main/java/com/demai/app/MainApplication.kt || true"
      },
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "prebuildCommand": "npm install && rm -rf android/app/src/main/java/com/demai/app/MainActivity.kt android/app/src/main/java/com/demai/app/MainApplication.kt || true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}